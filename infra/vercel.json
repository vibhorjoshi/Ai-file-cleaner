{
  "version": 2,
  "monorepo": true,
  "buildCommand": "pnpm run build --filter=@ai-file-cleanup/web",
  "outputDirectory": "apps/web/out",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://ai-cleanup-api.onrender.com/api/$1"
    }
  ],
  "env": {
    "API_BASE_URL": "https://ai-cleanup-api.onrender.com",
    "NEXT_PUBLIC_API_URL": "https://ai-cleanup-api.onrender.com",
    "SIMILARITY_THRESHOLD": "0.85",
    "MAX_FILE_SIZE": "104857600"
  }
}