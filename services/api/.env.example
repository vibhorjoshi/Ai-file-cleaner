# Environment Configuration
NODE_ENV=development
PORT=3001

# Database (Neon Postgres + pgvector)
DATABASE_URL=postgresql://username:password@ep-example.us-east-1.aws.neon.tech/ai_file_cleanup?sslmode=require

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
SESSION_SECRET=your-super-secret-session-key-change-in-production

# File Processing
MAX_FILE_SIZE=100MB
ALLOWED_MIME_TYPES=image/jpeg,image/png,image/gif,application/pdf,text/plain,text/csv

# AI Model Worker (future)
MODEL_WORKER_URL=http://localhost:3002

# Render.com deployment
RENDER=false